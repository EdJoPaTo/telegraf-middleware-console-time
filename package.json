{
	"name": "telegraf-middleware-console-time",
	"version": "2.1.0",
	"description": "Debug your Telegram middlewares",
	"keywords": [
		"analysis",
		"console",
		"debug",
		"middleware",
		"telegraf",
		"grammy",
		"telegram",
		"time",
		"timing"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/EdJoPaTo/telegraf-middleware-console-time.git"
	},
	"author": {
		"name": "EdJoPaTo",
		"email": "telegraf-npm-package@edjopato.de",
		"url": "https://edjopato.de"
	},
	"scripts": {
		"build": "del-cli dist && tsc",
		"prepack": "npm run build",
		"test": "tsc --sourceMap && xo && c8 --all node --test --enable-source-maps"
	},
	"type": "module",
	"engines": {
		"node": ">=18"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^6.0.0",
		"@types/node": "^22.5.4",
		"c8": "^10.1.2",
		"del-cli": "^5.0.0",
		"grammy": "^1.2.0",
		"telegraf": "^4.0.0",
		"typescript": "^5.5.2",
		"xo": "^0.59.3"
	},
	"files": [
		"dist",
		"!*.test.*",
		"!test.*"
	],
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"xo": {
		"rules": {
			"@typescript-eslint/ban-types": "off",
			"@typescript-eslint/naming-convention": "off",
			"@typescript-eslint/prefer-readonly-parameter-types": "warn",
			"unicorn/prevent-abbreviations": "off"
		},
		"overrides": [
			{
				"files": [
					"**/*.test.*",
					"**/test.*"
				],
				"rules": {
					"@typescript-eslint/prefer-readonly-parameter-types": "off",
					"n/no-unsupported-features/es-syntax": "off",
					"n/no-unsupported-features/node-builtins": "off"
				}
			}
		]
	}
}
