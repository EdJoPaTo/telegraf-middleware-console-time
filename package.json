{
	"name": "telegraf-middleware-console-time",
	"version": "1.0.1",
	"description": "Quick and dirty way to see whats incoming to your Telegraf Telegram bot while developing",
	"license": "MIT",
	"repository": "EdJoPaTo/telegraf-middleware-console-time",
	"author": {
		"name": "EdJoPaTo",
		"email": "edjopato@gmail.com",
		"url": "https://edjopato.de"
	},
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "del-cli dist && tsc",
		"prepack": "npm run build",
		"test": "tsc --sourceMap && xo"
	},
	"main": "dist",
	"types": "dist",
	"files": [
		"dist",
		"!*.test.*"
	],
	"keywords": [
		"analysis",
		"console",
		"debug",
		"middleware",
		"telegraf",
		"telegram",
		"time",
		"timing"
	],
	"dependencies": {},
	"peerDependencies": {
		"telegraf": "^3.38.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.7.0",
		"@types/node": "^13.1.4",
		"del-cli": "^3.0.0",
		"telegraf": "^3.38.0",
		"typescript": "^3.7.4",
		"xo": "^0.29.0"
	},
	"xo": {
		"semicolon": false,
		"rules": {
			"@typescript-eslint/prefer-readonly-parameter-types": "off"
		}
	}
}
