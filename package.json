{
	"name": "telegraf-middleware-console-time",
	"version": "2.0.0",
	"description": "Quick and dirty way to see whats incoming to your Telegraf Telegram bot while developing",
	"license": "MIT",
	"repository": "EdJoPaTo/telegraf-middleware-console-time",
	"author": {
		"name": "EdJoPaTo",
		"email": "telegraf-npm-package@edjopato.de",
		"url": "https://edjopato.de"
	},
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"build": "del-cli dist && tsc",
		"prepack": "npm run build",
		"test": "tsc --sourceMap && xo"
	},
	"main": "dist",
	"types": "dist",
	"files": [
		"dist",
		"!*.test.*"
	],
	"keywords": [
		"analysis",
		"console",
		"debug",
		"middleware",
		"telegraf",
		"telegram",
		"time",
		"timing"
	],
	"dependencies": {},
	"peerDependencies": {
		"telegraf": "^4.0.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.9.0",
		"@types/node": "^14.6.2",
		"del-cli": "^3.0.0",
		"telegraf": "^4.0.0",
		"typescript": "^4.1.2",
		"xo": "^0.38.1"
	},
	"xo": {
		"semicolon": false,
		"rules": {
			"@typescript-eslint/prefer-readonly-parameter-types": "warn"
		}
	}
}
