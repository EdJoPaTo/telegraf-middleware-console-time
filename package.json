{
	"name": "telegraf-middleware-console-time",
	"version": "2.1.0",
	"description": "Debug your Telegram middlewares",
	"keywords": [
		"analysis",
		"console",
		"debug",
		"middleware",
		"telegraf",
		"grammy",
		"telegram",
		"time",
		"timing"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/EdJoPaTo/telegraf-middleware-console-time.git"
	},
	"author": {
		"name": "EdJoPaTo",
		"email": "telegraf-npm-package@edjopato.de",
		"url": "https://edjopato.de"
	},
	"scripts": {
		"build": "del-cli dist && tsc",
		"prepack": "npm run build",
		"test": "tsc --sourceMap && xo && c8 --all ava"
	},
	"type": "module",
	"engines": {
		"node": ">=18"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^4.0.0",
		"ava": "^5.2.0",
		"c8": "^8.0.1",
		"del-cli": "^5.0.0",
		"grammy": "^1.2.0",
		"telegraf": "^4.0.0",
		"typescript": "^5.0.2",
		"xo": "^0.56.0"
	},
	"files": [
		"dist/source",
		"!*.test.*"
	],
	"main": "./dist/source/index.js",
	"types": "./dist/source/index.d.ts",
	"xo": {
		"rules": {
			"@typescript-eslint/ban-types": "off",
			"@typescript-eslint/naming-convention": "off",
			"@typescript-eslint/prefer-readonly-parameter-types": "warn",
			"ava/no-ignored-test-files": "off",
			"ava/no-skip-test": "off"
		}
	}
}
